{
  "info": {
    "name": "üñºÔ∏è Anastasia Gallery Module",
    "_postman_id": "anastasia-gallery-module",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "üñºÔ∏è Upload Image (Admin)",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
          }
        ],
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "image",
              "type": "file",
              "src": ""
            }
          ]
        },
        "url": {
          "raw": "{{url-anastasia}}/gallery/upload",
          "host": ["{{url-anastasia}}"],
          "path": ["gallery", "upload"]
        }
      }
    },
    {
      "name": "üìÇ Get All Gallery Items (Public)",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{url-anastasia}}/gallery",
          "host": ["{{url-anastasia}}"],
          "path": ["gallery"]
        }
      }
    },
    {
      "name": "üóëÔ∏è Delete Gallery Item (Admin)",
      "request": {
        "method": "DELETE",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
          }
        ],
        "url": {
          "raw": "{{url-anastasia}}/gallery/{{galleryItemId}}",
          "host": ["{{url-anastasia}}"],
          "path": ["gallery", "{{galleryItemId}}"]
        }
      }
    }
  ]
}
